Pokemon Charmander;
Pokemon Pikachu;
Pokemon Arbok;
Pokemon Kleytor;
Pokemon Umbreon;
Pokemon Snaxs;
PImage Inicio;
PImage Seleccion;
PImage Juego;
PImage TeclaA, TeclaS, TeclaD, TeclaF, TeclaG, TeclaH;
PImage Ganador1, Ganador2, Ganador3, Ganador4, Ganador5, Ganador6;
int Pantalla = 0;

void setup(){
  background(0);
  size(900,500);
  
  Inicio=loadImage("Inicio.png");
  Seleccion=loadImage("Seleccion.png");
  Juego=loadImage("Juego.png");
  TeclaA=loadImage("TeclaA.png");
  TeclaS=loadImage("TeclaS.png");
  TeclaD=loadImage("TeclaD.png");
  TeclaF=loadImage("TeclaF.png");
  TeclaG=loadImage("TeclaG.png");
  TeclaH=loadImage("TeclaH.png");
  Ganador1=loadImage("Ganador1.png");
  Ganador2=loadImage("Ganador2.png");
  Ganador3=loadImage("Ganador3.png");
  Ganador4=loadImage("Ganador4.png");
  Ganador5=loadImage("Ganador5.png");
  Ganador6=loadImage("Ganador6.png");
  
  //ELEMENTOS PRINCIPALES DE LA CLASE POKEMON.
  Charmander = new Pokemon("Charmander",1000,80,250,4,0,0);
  Pikachu = new Pokemon("Pikachu",1000,80,570,6,0,1);
  Arbok = new Pokemon("Arbok",1000,290,80,5,1,2);
  Kleytor = new Pokemon("Kleytor",1000,80,1370,3,3,3);
  Umbreon = new Pokemon("Umbreon",1000,80,500,6,1,4);
  Snaxs = new Pokemon("Snaxs",1000,80,390,5,2,5);
  
}

//DIVISIÓN DE LAS PANTALLAS.
void draw(){
  println(Pantalla);
  switch(Pantalla){
  case 0:
  image(Inicio,0,0);
  if (mousePressed == false){
  }else{
    Pantalla =1;
  }
  break;
  
  case 1:
  image(Seleccion,0,0);
  textSize(30);
  fill(0);
  text("SELECCIONA TU PERSONAJE",240,40);
  translate(0,35);
  fill(255,0,0,70);
  ellipse(160,120,150,150);
  fill(0);
  textSize(30);
  text("CHARMANDER",55,130);
  translate(85,60);
  fill(0);
      textSize(15); 
      text("Presiona A", 38, 119); 
      if (keyPressed){
      if (key == 'a' || key == 'A'){
      Pantalla +=1; 
   }
  }
  
  noStroke();
  fill(255,0,0,70);
  ellipse(350,65,150,150);
  fill(0);
  textSize(30);
  text("PIKACHU",285,70);
  translate(270,-38);
      fill(0);
      textSize(15); 
      text("Presiona S", 42,160); 
      if (keyPressed){
      if (key == 's' || key == 'S'){
      Pantalla +=2; 
   }
  }
  
  noStroke();
  fill(255,0,0,70);
  ellipse(380,110,150,150);
  fill(0);
  textSize(30);
  text("ARBOK",330,110);
  translate(310,20);
      fill(0);
      textSize(15); 
      text("Presiona D",30,140); 
      if (keyPressed){
      if (key == 'd' || key == 'D'){
      Pantalla +=3; 
   }
  }
  
  noStroke();
  fill(255,0,0,70);
  ellipse(-500,300,150,150);
  fill(0);
  textSize(30);
  text("KLEYTOR",-568,310);
  translate(-580,250);
      fill(0);
      textSize(15); 
      text("Presiona F",40,110); 
      if (keyPressed){
      if (key == 'f' || key == 'F'){
      Pantalla +=4;  
   }
  }
  
  noStroke();
  fill(255,0,0,70);
  ellipse(360,50,150,150);
  fill(0);
  textSize(30);
  text("UMBREON",288,60);
  translate(290,-20);
      fill(0);
      textSize(15); 
      text("Presiona G",35,130); 
      if (keyPressed){
      if (key == 'g' || key == 'G'){
      Pantalla +=5; 
   }
  }
  
  noStroke();
  fill(255,0,0,70);
  ellipse(370,70,150,150);
  fill(0);
  textSize(30);
  text("SNAXS",320,78);
  translate(290,0);
      fill(0);
      textSize(15); 
      text("Presiona H",40,130); 
      if (keyPressed){
      if (key == 'h' || key == 'H'){
      Pantalla +=6; 
   }
  }
  break;
  
//PRIMER DUELO
  case 2:
  image(TeclaA,0,0);
    fill(58,170,53);
    textSize(20);
    fill(58,170,53);
    text("HP "+" 100/100", 200, 440);
    textSize(20);
    fill(58,170,53);
    text("HP "+" 100/100",700,440);
    fill(0);
    textAlign(CENTER);
    textSize(30);
    text("Presiona Q para hacer daño", 450, 30);
  if (key == 'a' || key == 'A'){ 
     translate(500,0);
     fill(230,51,42);
     text("CHARMANDER",-300,100);
     fill(0);
     text("UMBREON",190,100);
  }
   if(key =='q' || key=='Q'){
    image(TeclaA,0,0);
    fill(58,170,53);
    textSize(20);
    fill(47,198,40);
    text("HP "+" 100/100", 200, 440);
    textSize(20);
    fill(47,198,40);
    text("HP "+" 75/100",700,440);
    fill(0);
    textAlign(CENTER);
    textSize(30);
    text("Presiona P para hacer daño", 450, 30);
    textSize(70);
    fill(47,198,40);
    }
   if(key =='p' || key=='P'){
    image(TeclaA,0,0);
    fill(58,170,53);
    textSize(20);
    fill(47,198,40);
    text("HP "+" 75/100", 200, 440);
    textSize(20);
    fill(47,198,40);
    text("HP "+" 75/100",700,440);
    fill(0);
    textAlign(CENTER);
    textSize(30);
    text("Presiona W para hacer daño", 450, 30);
    textSize(70);
    fill(47,198,40);
   }
   if(key =='w' || key=='W'){
    image(TeclaA,0,0);
    fill(58,170,53);
    textSize(20);
    fill(47,198,40);
    text("HP "+" 75/100", 200, 440);
    textSize(20);
    fill(47,198,40);
    text("HP "+" 50/100",700,440);
    fill(0);
    textAlign(CENTER);
    textSize(30);
    text("Presiona O para hacer daño", 450, 30);
    textSize(70);
    fill(47,198,40);
   }
   if(key =='o' || key=='O'){
    image(TeclaA,0,0);
    fill(58,170,53);
    textSize(20);
    fill(47,198,40);
    text("HP "+" 50/100", 200, 440);
    textSize(20);
    fill(47,198,40);
    text("HP "+" 50/100",700,440);
    fill(0);
    textAlign(CENTER);
    textSize(30);
    text("Presiona E para hacer daño", 450, 30);
    textSize(70);
    fill(47,198,40);
   }
   if(key =='e' || key=='E'){
    image(TeclaA,0,0);
    fill(58,170,53);
    textSize(20);
    fill(47,198,40);
    text("HP "+" 50/100", 200, 440);
    textSize(20);
    fill(47,198,40);
    text("HP "+" 25/100",700,440);
    fill(0);
    textAlign(CENTER);
    textSize(30);
    text("Presiona I para hacer daño", 450, 30);
    textSize(70);
    fill(47,198,40);
   }
   if(key =='i' || key=='I'){
    image(TeclaA,0,0);
    fill(58,170,53);
    textSize(20);
    fill(47,198,40);
    text("HP "+" 25/100", 200, 440);
    textSize(20);
    fill(47,198,40);
    text("HP "+" 25/100",700,440);
    fill(0);
    textAlign(CENTER);
    textSize(30);
    text("Presiona R para hacer daño", 450, 30);
    textSize(70);
    fill(47,198,40);
   }
   if(key =='r' || key=='R'){
    image(Ganador1,0,0);
    fill(58,170,53);
    textSize(20);
    fill(47,198,40);
    text("HP "+" 25/100", 200, 440);
    textSize(20);
