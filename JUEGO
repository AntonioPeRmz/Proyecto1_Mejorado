void seleccion(){

}
class Pokemon {
  String nombre;
  int hp;
  int ataque;
  int defensa;
  int elemento;
  int evolucion;
  int dibujo;
  
  
  Pokemon(String nombre_, int hp_, int ataque_, int defensa_, int elemento_, int evolucion_, int dibujo_){
    nombre = nombre_;
    hp = hp_;
    ataque = ataque_;
    defensa = defensa_;
    elemento = elemento_;
    evolucion = evolucion_;
    dibujo = dibujo_;  
}

