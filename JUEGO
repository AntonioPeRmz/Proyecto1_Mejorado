    text("HP "+" 0/100",700,440);
    fill(0);
    textAlign(CENTER);
    textSize(70);
    fill(47,198,40);
    textSize(70);
    fill(47,198,40);
    text("GANASTE EL JUEGO",465,240);
    textSize(20);
    fill(255);
    text("PRESIONA Y PARA REINICIAR",465,280);
   }
